{
    "autoclose": true,
    "autoindent": true,
    "autosave": 0,
    "autosu": true,
    "backup": false,
    "basename": true,
    "colorcolumn": 0,
    "colorscheme": "darcula",
    "comment": true,
    "cursorline": true,
    "diff": true,
    "diffgutter": false,
    "encoding": "utf-8",
    "eofnewline": false,
    "fastdirty": true,
    "filemanager": true,
    "filemanager.compressparent": true,
    "filemanager.foldersfirst": true,
    "filemanager.openonstart": false,
    "filemanager.showdotfiles": true,
    "filemanager.showignored": true,
    "filetype": "unknown",
    "ftoptions": true,
    "ignorecase": false,
    "indentchar": " ",
    "infobar": true,
    "initlua": true,
    "keepautoindent": false,
    "keymenu": false,
    "linter": true,
    "literate": true,
    "lsp": true,
    "lsp.autocompleteDetails": true,
    "lsp.formatOnSave": false,
    "lsp.ignoreMessages": " ",
	"lsp.ignoreTriggerCharacters": "completion,signature",
    "lsp.server": "python=pylsp,c=clangd, go=gopls,typescript=deno lsp,javascript=deno lsp,markdown=deno lsp,json=deno lsp,jsonc=deno lsp,rust=rls,lua=lua-lsp,c++=clangd",
    "lsp.tabcompletion": true,
    "matchbrace": true,
    "mkparents": false,
    "mouse": true,
    "paste": false,
    "pluginchannels": [
        "https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json"
    ],
    "pluginrepos": [],
    "readonly": false,
    "rmtrailingws": false,
    "ruler": true,
    "savecursor": false,
    "savehistory": true,
    "saveundo": false,
    "scrollbar": true,
    "scrollmargin": 3,
    "scrollspeed": 2,
    "smartpaste": true,
    "softwrap": false,
    "splitbottom": true,
    "splitright": true,
    "status": true,
    "statusformatl": "$(filename) ($(line),$(col)) $(status.paste)| ft:$(opt:filetype) | $(opt:fileformat) | $(opt:encoding)",
    "statusformatr": "",
    "statusline": true,
    "sucmd": "sudo",
    "syntax": true,
    "tabmovement": false,
    "tabsize": 4,
    "tabstospaces": false,
    "useprimary": true
